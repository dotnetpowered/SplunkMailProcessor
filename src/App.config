<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
      <!-- NOTE: Be sure to remove System.Net.Http bindings from the generated exe.config or you'll get an error -->
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <appSettings>
    <add key="Splunk.ApiKey" value=""/>
    <add key="Splunk.Url" value="http://server-name:8088/services/collector/raw"/>
    <add key="Exchange.User" value="user@domain.com"/>
    <add key="Exchange.AppId" value="" />
    <add key="Exchange.TenantId" value="" />
    <add key="Exchange.ClientSecret" value="" />
    <add key="Exchange.Url" value="https://outlook.office365.com/EWS/Exchange.asmx" />
    <add key="Exchange.MessagesPerPoll" value="100" />
  </appSettings>
</configuration>